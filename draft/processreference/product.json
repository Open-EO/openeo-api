{
    "id": "product",
    "summary": "Multiplication of a sequence of numbers",
    "description": "This process is an exact alias for the `multiply` process. See ``multiply()`` for more information.",
    "categories": [
        "math",
        "reducer"
    ],
    "parameter_order": [
        "data",
        "ignore_nodata"
    ],
    "parameters": {
        "data": {
            "description": "See ``multiply()`` for more information.",
            "schema": {
                "type": "array",
                "items": {
                    "type": [
                        "number",
                        "null"
                    ]
                }
            },
            "required": true
        },
        "ignore_nodata": {
            "description": "See ``multiply()`` for more information.",
            "schema": {
                "type": "boolean"
            },
            "default": true
        }
    },
    "returns": {
        "description": "See ``multiply()`` for more information.",
        "schema": {
            "type": [
                "number",
                "null"
            ]
        }
    },
    "exceptions": {
        "MultiplicandMissing": {
            "message": "Multiplication requires at least two numbers."
        }
    },
    "examples": [
        {
            "arguments": {
                "data": [
                    5,
                    0
                ]
            },
            "returns": 0
        },
        {
            "arguments": {
                "data": [
                    -2,
                    4,
                    2.5
                ]
            },
            "returns": -20
        },
        {
            "arguments": {
                "data": [
                    1,
                    null
                ],
                "ignore_nodata": false
            },
            "returns": null
        }
    ]
}